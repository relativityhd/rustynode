{
  "name": "rustynode",
  "version": "1.0.0",
  "description": "Benchmarking for Rust Language integrations into NodeJS",
  "main": "src/server.js",
  "scripts": {
    "start": "node -r esm ./src/server.js",
    "dev": "nodemon -r esm ./src/server.js",
    "test": "node -r esm ./src/tests/tests.js",
    "preinstall": "npm i --prefix ./crates/napi-rs && npm i --prefix ./crates/neon && npm i --prefix ./crates/node-bindgen && npm i --prefix ./crates/napi && npm i --prefix ./crates/wasm",
    "build-napi": "cd ./crates/napi && cargo build --release && mv ./target/release/libnapi.dylib ./index.node",
    "build-napirs": "cd ./crates/napi-rs && cargo build --release && napi build --release",
    "build-wasm": "cd ./crates/wasm && wasm-pack build --target nodejs --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relativityhd/rustynode.git"
  },
  "keywords": [
    "rust",
    "node",
    "wasm"
  ],
  "author": "Tobias Hoelzer <tobiashoelzer@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relativityhd/rustynode/issues"
  },
  "homepage": "https://github.com/relativityhd/rustynode#readme",
  "dependencies": {
    "benchmark": "^2.1.4",
    "express": "^4.17.1",
    "json-to-markdown-table": "^1.0.0",
    "microtime": "^3.0.0",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "napi-rs": "^0.3.1",
    "wasm-pack": "^0.9.1"
  },
  "standard": {
    "ignoreRules": [
      "camelcase"
    ]
  }
}
